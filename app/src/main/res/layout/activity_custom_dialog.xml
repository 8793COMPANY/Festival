<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/custom_dialog_background"
    tools:context=".dialog.CustomDialog">

    <ImageView
        android:id="@+id/dialogImage"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/custom_dialog_image"
        android:layout_marginTop="10dp"
        app:layout_constraintWidth_percent="0.205"
        app:layout_constraintHeight_percent="0.111"
        app:layout_constraintVertical_bias="0.104"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/dialogText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/pretendard_medium"
        android:textColor="#ff5b8f"
        android:textSize="19dp"
        android:text="예약완료"
        app:layout_constraintVertical_bias="0.026"
        app:layout_constraintTop_toBottomOf="@+id/dialogImage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/dialogDateText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/pretendard_regular"
        android:textColor="#ffffff"
        android:textSize="15dp"
        app:layout_constraintVertical_bias="0.112"
        app:layout_constraintTop_toBottomOf="@+id/dialogText"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/dialogNumText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/pretendard_regular"
        android:textColor="#ffffff"
        android:textSize="15dp"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintTop_toBottomOf="@+id/dialogDateText"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/dialogCheckText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/pretendard_regular"
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:text="마이페이지>예약내역에서\nQR코드를 확인해주세요"
        android:gravity="center"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintTop_toBottomOf="@+id/dialogNumText"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageView
        android:id="@+id/lineImage"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#3b3848"
        app:layout_constraintVertical_bias="0.296"
        app:layout_constraintTop_toBottomOf="@+id/dialogCheckText"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/okButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/pretendard_medium"
        android:textColor="#d9d9d9"
        android:textSize="19dp"
        android:text="예약확인"
        style="?android:attr/borderlessButtonStyle"
        app:layout_constraintWidth_percent="0.505"
        app:layout_constraintHeight_percent="0.211"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0.296"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/closeButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/pretendard_medium"
        android:textColor="#ff44a2"
        android:textSize="19dp"
        android:text="닫기"
        style="?android:attr/borderlessButtonStyle"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintWidth_percent="0.505"
        app:layout_constraintHeight_percent="0.211"
        app:layout_constraintVertical_bias="0.296"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageView
        android:id="@+id/lineImage2"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="#3b3848"
        app:layout_constraintHeight_percent="0.211"
        app:layout_constraintTop_toBottomOf="@+id/lineImage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>